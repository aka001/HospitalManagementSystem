<table>
	<tr>
		<th>Username</th>
		<td><%= f.text_field(:username) %></td>
	</tr>
	<tr>
		<th>Password</th>
		<td><%= f.password_field(:password) %></td>
	</tr>
	<tr>
		<th>Name</th>
		<td><%= f.text_field(:name) %></td>
	</tr>
	<tr>
		<th>Gender</th>
		<td><%= f.select(:gender,['M','F']) %></td>
	</tr>
	<tr>
		<th>Date of Birth</th>
		<td><%= f.datetime_select(:date_of_birth) %></td>
	</tr>
	<tr>
		<th>Address</th>
		<td><%= f.text_area(:address) %></td>
	</tr>
	<tr>
		<th>Qualification</th>
		<td><%= f.text_field(:qualification) %></td>
	</tr>
	<tr>
		<th>Education</th>
		<td><%= f.text_field(:education) %></td>
	</tr>
	<tr>
		<th>Specialised Fields</th>
		<td><%= f.text_field(:specialised_fields) %></td>
	</tr>
	<tr>
		<th>Experience Years</th>
		<td><%= f.text_field(:experience_years) %></td>
	</tr>
	<tr>
		<th>Salary</th>
		<td><%= f.text_field(:salary) %></td>
	</tr>
</table>
