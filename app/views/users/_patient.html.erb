<table>
	<tr>
		<th>Username</th>
		<td><%= f.text_field(:username) %></td>
	</tr>
	<tr>
		<th>Password</th>
		<td><%= f.password_field(:password) %></td>
	</tr>
	<tr>
		<th>Name</th>
		<td><%= f.text_field(:name) %></td>
	</tr>
	<tr>
		<th>Gender</th>
		<td><%= f.select(:gender,['M','F']) %></td>
	</tr>
	<tr>
		<th>age</th>
		<td><%= f.text_field(:age,:maxlength=>3) %></td>
	</tr>
	<tr>
		<th>address</th>
		<td><%= f.text_area(:address) %></td>
	</tr>
	<tr>
		<th>contact_number</th>
		<td><%= f.text_field(:qualification,:maxlength=>11) %></td>
	</tr>
</table>
