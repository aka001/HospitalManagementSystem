<table>
	<tr>
		<th>Username</th>
		<td><%= f.text_field(:username) %></td>
	</tr>
	<tr>
		<th>Password</th>
		<td><%= f.password_field(:password) %></td>
	</tr>
	<tr>
		<th>Name</th>
		<td><%= f.text_field(:name) %></td>
	</tr>
	<tr>
		<th>Gender</th>
		<td><%= f.select(:gender,['M','F']) %></td>
	</tr>
	<tr>
		<th>Age</th>
		<td><%= f.text_field(:age,:maxlength=>3) %></td>
	</tr>
	<tr>
		<th>Address</th>
		<td><%= f.text_area(:address) %></td>
	</tr>
	<tr>
		<th>Contact Number</th>
		<td><%= f.text_field(:contact_number,:maxlength=>13) %></td>
	</tr>
</table>
